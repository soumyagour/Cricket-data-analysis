      "cell_type": "markdown",
      "source": [
        "(2) Process Batting Summary"
      ],
      "metadata": {
        "id": "PQKqZxoUGjdJ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "with open('/t20_wc_batting_summary.json') as f:\n",
        "    data = json.load(f)\n",
        "    all_records = []\n",
        "    for rec in data:\n",
        "        all_records.extend(rec['battingSummary'])\n",
        "  \n",
        "df_batting = pd.DataFrame(all_records)\n",
        "df_batting.head(11)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 394
